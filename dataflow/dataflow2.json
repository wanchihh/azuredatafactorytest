{
	"name": "dataflow2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "CosmosDbSqlApiSampleContainerDS",
						"type": "DatasetReference"
					},
					"name": "Address"
				},
				{
					"dataset": {
						"referenceName": "AzureSqlTablePersonDS",
						"type": "DatasetReference"
					},
					"name": "person"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "AzureSqlTablePersonAddress",
						"type": "DatasetReference"
					},
					"name": "personaddress"
				}
			],
			"transformations": [
				{
					"name": "join1"
				},
				{
					"name": "RemoveDupIdColumn",
					"description": "Autogenerated by data preview actions"
				}
			],
			"scriptLines": [
				"source(output(",
				"          address as string,",
				"          id as string",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     query: 'select c.id, c.address from c',",
				"     format: 'documentQuery',",
				"     systemColumns: false) ~> Address",
				"source(output(",
				"          id as string,",
				"          surname as string,",
				"          given_name as string,",
				"          age as integer,",
				"          gender as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> person",
				"Address, person join(Address@id == person@id,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1 select(mapColumn(",
				"          address,",
				"          id,",
				"          surname,",
				"          given_name,",
				"          age,",
				"          gender",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> RemoveDupIdColumn",
				"RemoveDupIdColumn sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id as string,",
				"          surname as string,",
				"          given_name as string,",
				"          age as integer,",
				"          gender as string,",
				"          address as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> personaddress"
			]
		}
	}
}