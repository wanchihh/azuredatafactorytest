{
	"name": "dataflow2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "CosmosDbSqlApiSampleContainerDS",
						"type": "DatasetReference"
					},
					"name": "Address"
				},
				{
					"dataset": {
						"referenceName": "AzureSqlTablePersonDS",
						"type": "DatasetReference"
					},
					"name": "person"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "AzureSqlTablePersonAddress",
						"type": "DatasetReference"
					},
					"name": "personaddress"
				}
			],
			"transformations": [
				{
					"name": "join1"
				},
				{
					"name": "RemoveDupIdColumn",
					"description": "Autogenerated by data preview actions"
				},
				{
					"name": "AlterRow1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          address as string,",
				"          id as string",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     query: 'select c.id, c.address from c',",
				"     format: 'documentQuery',",
				"     systemColumns: false) ~> Address",
				"source(output(",
				"          id as string,",
				"          surname as string,",
				"          given_name as string,",
				"          age as integer,",
				"          gender as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> person",
				"Address, person join(Address@id == person@id,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1 select(mapColumn(",
				"          address,",
				"          id = person@id,",
				"          surname,",
				"          given_name,",
				"          age,",
				"          gender",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> RemoveDupIdColumn",
				"derivedColumn1 alterRow(upsertIf(true())) ~> AlterRow1",
				"RemoveDupIdColumn derive(isOver25 = age > 25) ~> derivedColumn1",
				"AlterRow1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id as string,",
				"          surname as string,",
				"          given_name as string,",
				"          age as integer,",
				"          gender as string,",
				"          address as string,",
				"          isOver25 as boolean",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:true,",
				"     keys:['id'],",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          id,",
				"          surname,",
				"          given_name,",
				"          age,",
				"          gender,",
				"          address,",
				"          isOver25",
				"     )) ~> personaddress"
			]
		}
	}
}