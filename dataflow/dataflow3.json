{
	"name": "dataflow3",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "CosmosDbSqlApiPoi",
						"type": "DatasetReference"
					},
					"name": "Poi"
				},
				{
					"dataset": {
						"referenceName": "AzureSqlBlacklist",
						"type": "DatasetReference"
					},
					"name": "Blacklist"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "AzureSqlBlacklistPoi",
						"type": "DatasetReference"
					},
					"name": "BlacklistPoi"
				}
			],
			"transformations": [
				{
					"name": "join1"
				},
				{
					"name": "RemoveColumns1",
					"description": "Autogenerated by data preview actions"
				}
			],
			"scriptLines": [
				"source(output(",
				"          date as string,",
				"          id as string,",
				"          location as string,",
				"          name as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     query: (\"select c.id, c.name, c.location, c.date from c\"),",
				"     format: 'documentQuery',",
				"     systemColumns: false) ~> Poi",
				"source(output(",
				"          id as string,",
				"          reason as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Blacklist",
				"Poi, Blacklist join(Poi@id == Blacklist@id,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1 select(mapColumn(",
				"          date,",
				"          location,",
				"          name,",
				"          id = Poi@id,",
				"          reason",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> RemoveColumns1",
				"RemoveColumns1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id as string,",
				"          name as string,",
				"          reason as string,",
				"          address as string,",
				"          location as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> BlacklistPoi"
			]
		}
	}
}