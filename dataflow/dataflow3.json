{
	"name": "dataflow3",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "CosmosDbSqlApiPoi",
						"type": "DatasetReference"
					},
					"name": "Poi"
				},
				{
					"dataset": {
						"referenceName": "AzureSqlBlacklist",
						"type": "DatasetReference"
					},
					"name": "Blacklist"
				},
				{
					"dataset": {
						"referenceName": "AzureSqlTableAddress",
						"type": "DatasetReference"
					},
					"name": "Address"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "AzureSqlBlacklistPoi",
						"type": "DatasetReference"
					},
					"name": "BlacklistPoi"
				}
			],
			"transformations": [
				{
					"name": "joinBlacklist"
				},
				{
					"name": "removeDuplicateIdCol",
					"description": "Autogenerated by data preview actions"
				},
				{
					"name": "convertDate"
				},
				{
					"name": "forUpsert"
				},
				{
					"name": "joinAddress"
				}
			],
			"scriptLines": [
				"source(output(",
				"          date as string,",
				"          id as string,",
				"          location as string,",
				"          name as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     query: (\"select c.id, c.name, c.location, c.date from c\"),",
				"     format: 'documentQuery',",
				"     systemColumns: false) ~> Poi",
				"source(output(",
				"          id as string,",
				"          reason as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Blacklist",
				"source(output(",
				"          id as string,",
				"          address as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Address",
				"Poi, Blacklist join(Poi@id == Blacklist@id,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> joinBlacklist",
				"joinAddress select(mapColumn(",
				"          date,",
				"          location,",
				"          name,",
				"          id = Poi@id,",
				"          reason",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> removeDuplicateIdCol",
				"removeDuplicateIdCol derive(poidate = toTimestamp(date, 'yyyy-MM-dd\\'T\\'HH:mm:ss.SSS')) ~> convertDate",
				"convertDate alterRow(upsertIf(true())) ~> forUpsert",
				"joinBlacklist, Address join(Poi@id == Address@id,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> joinAddress",
				"forUpsert sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id as string,",
				"          name as string,",
				"          reason as string,",
				"          address as string,",
				"          location as string,",
				"          poidate as timestamp",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:true,",
				"     keys:['id','poidate'],",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          id,",
				"          name,",
				"          reason,",
				"          location,",
				"          poidate",
				"     )) ~> BlacklistPoi"
			]
		}
	}
}